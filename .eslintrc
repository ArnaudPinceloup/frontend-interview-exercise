{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "env": {
    "jest": true
  },
  "globals": {
    "document": false,
    "window": false,
    "fetch": false,
    "require": false,
    "alert": false,
    "FileReader": false
  },
  "parser": "babel-eslint"
}
